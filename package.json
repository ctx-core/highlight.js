{
	"name": "@ctx-core/highlight.js",
	"version": "5.2.117",
	"description": "ctx-core highlight.js",
	"keywords": [
		"ctx-core",
		"highlight.js"
	],
	"homepage": "https://github.com/ctx-core/highlight.js#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/highlight.js/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/highlight.js.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./initHighlighting__refresh"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test:unit && pnpm test:types",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"@ctx-core/dom": "^15.3.6",
		"ctx-core": "^5.30.0",
		"highlight.js": "^11.9.0"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}