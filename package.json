{
	"name": "@ctx-core/highlight.js",
	"version": "5.0.35",
	"description": "ctx-core highlight.js",
	"keywords": [
		"ctx-core",
		"highlight.js"
	],
	"homepage": "https://github.com/ctx-core/highlight.js#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/highlight.js/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/highlight.js/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./src/index.ts",
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/dom": "workspace:^11.0.40",
		"@ctx-core/function": "workspace:^20.2.12"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"highlight.js": "^11.3.1",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
